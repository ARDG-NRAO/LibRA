include: htclean.params.htc

executable = htclean_condor.sh
arguments  = "'$(python_state)' '$(input_file)' '$(partId)' '$(msname)' '$(imagename)' '$(cfcache)' '$(initialdir)'"


output = $(python_state).imcycle$(imcycle).stdout
error  = $(python_state).imcycle$(imcycle).stderr
log    = $(python_state).imcycle$(imcycle).condor.log


request_cpus = 1


+WantIOProxy = True

