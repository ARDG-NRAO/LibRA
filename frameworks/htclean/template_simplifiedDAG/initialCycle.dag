JOB               makePSF            makePSF.htc             
JOB               makePB             makePB.htc              
JOB               gatherPSF          gatherPSF.htc          
JOB               makeDirtyImage     makeDirtyImage.htc      
JOB               gather             gather.htc              


VARS              makePSF            python_state="$(JOB)"
VARS              makePSF            input_file="../bin/htclean.params"
VARS              makePSF            partId="n"

VARS              makeDirtyImage     python_state="runResidualCycle"
VARS              makeDirtyImage     input_file="../bin/htclean.params"
VARS              makeDirtyImage     partId="n"

VARS              gatherPSF          python_state="$(JOB)"
VARS              gatherPSF          input_file="../bin/htclean.params"
VARS              gatherPSF          partId="n"

VARS              gather             python_state="$(JOB)"
VARS              gather             input_file="../bin/htclean.params"
VARS              gather             partId="n"

VARS              makePB             python_state="$(JOB)"
VARS              makePB             input_file="../bin/htclean.params"
VARS              makePB             partId="n"


PARENT            makePSF            CHILD makePB
PARENT            makePB             CHILD gatherPSF
PARENT            gatherPSF          CHILD makeDirtyImage
PARENT            makeDirtyImage     CHILD gather
